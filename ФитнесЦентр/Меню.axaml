<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ФитнесЦентр.Меню"
        Title="Меню">
        <Grid>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Button Name="Enter" Content="Вход" Click="OpenAutorization" Margin=" 10 10 15 10"></Button>
                        <Button Name="Open" Content="Центр" Click="OpenWindow" Margin=" 10 10 15 10"></Button>
                        <Button Name="Exit" Content="Выход" Click="Exit" Margin=" 10 10 15 10"></Button>
                </StackPanel>  
        </Grid>
</Window>
